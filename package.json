{
  "name": "open-redistricting",
  "version": "0.0.1",
  "description": "Moving the redistricting process out into the open. A US Open Data + Stamen joint.",
  "author": "ericsoco",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:stamen/open-redistricting.git"
  },
  "engines": {
    "yarn": "^1.13.0"
  },
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "gh-deploy": "yarn dist && git-directory-deploy --directory dist/",
    "start:old": "yarn sass && yarn run-p watch serve",
    "dist:old": "yarn js && yarn sass && yarn static",
    "watch:old": "yarn js:watch & yarn sass:watch & yarn static:watch",
    "serve:old": "serve -l 3000 dist",
    "js:old": "browserify js/main.jsx -t [ loose-envify --NODE_ENV production ] > dist/bundle.js",
    "js:watch:old": "watchify js/main.jsx -o dist/bundle.js -dv",
    "sass:old": "node-sass --include-path scss scss/main.scss dist/main.css",
    "sass:watch:old": "node-sass -w --include-path scss scss/main.scss dist/main.css",
    "static:old": "cp -R static/ dist/",
    "static:watch:old": "nodemon -w static -e html,json,csv,gif,jpg,png,svg -x \"yarn static:old\""
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.7",
    "babel-loader": "^8.0.5",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "git-directory-deploy": "^1.5.1",
    "html-webpack-plugin": "^3.2.0",
    "loose-envify": "^1.4.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "serve": "^10.1.1",
    "style-loader": "^0.23.1",
    "watchify": "^3.11.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "babel-core": "^7.0.0-bridge.0",
    "isomorphic-fetch": "^2.2.1",
    "jsts": "^2.0.3",
    "leaflet": "^1.3.4",
    "lodash.get": "^4.4.2",
    "moment": "^2.14.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-leaflet": "^2.1.4",
    "react-modal": "^3.8.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "sanitize-html": "^1.20.0",
    "slug": "^0.9.3"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
